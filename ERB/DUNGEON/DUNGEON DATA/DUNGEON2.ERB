@DUNGEON2_INIT
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC LCOUNT2
#DIMS DYNAMIC MAPDATAS,19

MAPDATAS:0  '= "0000000000000000000"
MAPDATAS:1  '= "0111111111111111110"
MAPDATAS:2  '= "0111111111111111110"
MAPDATAS:3  '= "0111111111111111110"
MAPDATAS:4  '= "0111111111111111110"
MAPDATAS:5  '= "0111111111111111110"
MAPDATAS:6  '= "0111111111111111110"
MAPDATAS:7  '= "0111111111111111110"
MAPDATAS:8  '= "0111111111111111110"
MAPDATAS:9  '= "011111111111111111>"
MAPDATAS:10 '= "0000000000005000110"
MAPDATAS:11 '= "01111110151^4110110"
MAPDATAS:12 '= "0111111[10114110110"
MAPDATAS:13 '= "0555555010[02010020"
MAPDATAS:14 '= "05*5555011101011110"
MAPDATAS:15 '= "0111111001001001140"
MAPDATAS:16 '= "0111101401014101110"
MAPDATAS:17 '= "0115111001031301130"
MAPDATAS:18 '= "000)00000)000000000"

FOR LCOUNT2,0,19
    FOR LCOUNT,0,19
        IF SUBSTRING(MAPDATAS:LCOUNT2,LCOUNT,1) == " "
            D3D_TOTAL_MAPDATA:2:LCOUNT:LCOUNT2 = -1
        ELSEIF SUBSTRING(MAPDATAS:LCOUNT2,LCOUNT,1) == "["
            D3D_TOTAL_MAPDATA:2:LCOUNT:LCOUNT2 = 32
        ELSEIF SUBSTRING(MAPDATAS:LCOUNT2,LCOUNT,1) == "*"
            D3D_TOTAL_MAPDATA:2:LCOUNT:LCOUNT2 = 14
        ELSEIF SUBSTRING(MAPDATAS:LCOUNT2,LCOUNT,1) == "^"
            D3D_TOTAL_MAPDATA:2:LCOUNT:LCOUNT2 = 24
        ELSEIF SUBSTRING(MAPDATAS:LCOUNT2,LCOUNT,1) == ")"
            D3D_TOTAL_MAPDATA:2:LCOUNT:LCOUNT2 = 29
        ELSEIF SUBSTRING(MAPDATAS:LCOUNT2,LCOUNT,1) == ">"
            D3D_TOTAL_MAPDATA:2:LCOUNT:LCOUNT2 = 39
        ELSE
            D3D_TOTAL_MAPDATA:2:LCOUNT:LCOUNT2 = TOINT(SUBSTRING(MAPDATAS:LCOUNT2,LCOUNT,1))
        ENDIF
    NEXT
NEXT

