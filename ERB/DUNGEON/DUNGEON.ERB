;현재 인수를 사용하지 않음
@DUNGEON_START(DUNGEONNUM)
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC LCOUNT2
#DIM DYNAMIC DUNGEONNUM

;이하 테스트용 맵데이터 하드코딩
;[SKIPSTART]
#DIMS DYNAMIC MAPDATAS,6
D3D_CURRENT_MAPXSIZE = 6
D3D_CURRENT_MAPYSIZE = 6

MAPDATAS:0 '= "000000"
MAPDATAS:1 '= "011000"
MAPDATAS:2 '= "011210"
MAPDATAS:3 '= "011010"
MAPDATAS:4 '= "000000"
MAPDATAS:5 '= "000000"

FOR LCOUNT2,0,6
    FOR LCOUNT,0,6
        D3D_CURRENT_MAPDATA:LCOUNT:LCOUNT2 = TOINT(SUBSTRING(MAPDATAS:LCOUNT2,LCOUNT,1))
    NEXT
NEXT

D3D_PLAYER_XCOORD = 2
D3D_PLAYER_YCOORD = 2
D3D_PLAYER_DIRECTION = 3
;[SKIPEND]

DO
    CALL D3D_SHOW_GRAPHIC
    CALL D3D_SHOW_COMMAND
    CALL D3D_INPUT_COMMAND
;SIF RESULT:0
;    턴수 += 1
LOOP 1

@DUNGEON_OPEN_DOOR(XCOORD,YCOORD)
#DIM DYNAMIC XCOORD
#DIM DYNAMIC YCOORD

SIF D3D_CURRENT_MAPDATA:XCOORD:YCOORD != 2
    RETURN 0
D3D_CURRENT_MAPDATA:XCOORD:YCOORD = 12
RETURN 1