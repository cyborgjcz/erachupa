@KOJO_BATTLE_WIN_K50
#DIMS DYNAMIC LNAME
#DIMS DYNAMIC LTEXT
#DIM DYNAMIC FACEV
LNAME = %CALLNAME:[[줄리아]]%
IF (TFLAG:불리전투회화스위치 & 1) != 0 || (TFLAG:불리전투회화스위치 & 2) != 0
    ;체력이 상당히 떨어짐
    LTEXT = 이 정도는 아무것도 아니지!
    FACEV = 5
    TFLAG:전투회화키워드1 = 5
    TFLAG:전투회화키워드2 = 2
ELSEIF TFLAG:전투경과시간회화 > 1
    ;전투에서 시간을 상당히 오래 끔
    LTEXT = 본래 실력은 이 정도가 아닌데...
    FACEV = 2
    TFLAG:전투회화키워드1 = 4
ELSEIF TFLAG:경과시간 < 60000
    ;1분 컷
    LTEXT = 하늘하늘한 의상 입는 스케줄보다도 이쪽이 간단한데?
    FACEV = 5
    TFLAG:전투회화키워드1 = 3
ELSE
    ;통상
    LTEXT = 짜자잔~ %CALLNAME:[[줄리아]]% 님의 승리! 핫하!
    FACEV = 5
    TFLAG:전투회화키워드1 = 2
ENDIF
CALL TALK_WITH_IMG(FACE([[줄리아]],FACEV),LNAME,LTEXT)
WAITANYKEY
RETURN 1
@KOJO_BATTLE_WIN_RES_K50
#DIMS DYNAMIC LNAME
#DIMS DYNAMIC LTEXT
#DIMS DYNAMIC LTEXT2
#DIM DYNAMIC FACEV = 1
LNAME = %CALLNAME:[[줄리아]]%
IF (TFLAG:불리전투회화스위치 & 1) != 0 || (TFLAG:불리전투회화스위치 & 2) != 0
    SELECTCASE TFLAG:전투회화키워드1
    ;체력이 상당히 떨어짐
        CASE 1
            ;걱정
            LTEXT = 난 괜찮아, %BUDDY([[줄리아]])%
            FACEV = 3
        CASE 7
            ;분함
            LTEXT = 내 능력은 아직 이 정도인가, 제길!
            FACEV = 5
        CASEELSE
            LTEXT = %MIXELLIPSIS(BUDDY([[줄리아]]))%...
            FACEV = 3
            ;기타
    ENDSELECT
ELSEIF TFLAG:전투경과시간회화 > 1
    ;전투에서 시간을 상당히 오래 끔
    SELECTCASE TFLAG:전투회화키워드1
        CASE 5
            ;피로
            LTEXT = 제길, 만만한 녀석이 없네.
            FACEV = 4
        CASEELSE
            LTEXT = 쫓아오는 게 있을지도 몰라.
            FACEV = 1
            ;기타
    ENDSELECT
ELSEIF TFLAG:경과시간 < 60000
    ;1분 컷
    SELECTCASE TFLAG:전투회화키워드1
        CASE 3
            ;장난
            LTEXT = 한 주먹거리도 안 되네!
            FACEV = 2
        CASEELSE
            LTEXT = 좋아, 다음!
            FACEV = 2
            ;기타
    ENDSELECT
ELSE
    ;통상
    SELECTCASE TFLAG:전투회화키워드1
        CASE 6
            ;공복
            FACEV = 2
            IF PARTNER([[줄리아]]) == [[시즈카]]
                LTEXT = 돌아가면 맛있는 우동집에 가자고.
            ELSE
                LTEXT = 하하, 그건 어쩔 수 없네.
            ENDIF
        CASEELSE
            FACEV = 1
            LTEXT = 또 쓸모없는 살생을 저질러버렸군...
            ;기타
    ENDSELECT
ENDIF
CALL TALK_WITH_IMG(FACE([[줄리아]],FACEV),LNAME,LTEXT)
WAITANYKEY
RETURN 1
