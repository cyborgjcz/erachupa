#DIMS DYNAMIC LNAME
#DIMS DYNAMIC LTEXT
LNAME = %CALLNAME:[[시즈카]]%
IF FLAG:탐색대상던전 == 1 && DUNGEON1_FLAG:3 == 0
    ;FIRST BATTLE
    LTEXT = 이것들은 뭐지?!
    CALL TALK_WITH_IMG(FACE([[시즈카]],4),LNAME,LTEXT)
    WAITANYKEY
    RETURN 1
