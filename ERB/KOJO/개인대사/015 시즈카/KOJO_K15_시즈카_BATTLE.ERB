@KOJO_BATTLE_WIN_K15
#DIMS DYNAMIC LNAME
#DIMS DYNAMIC LTEXT
#DIM DYNAMIC FACEV
LNAME = %CALLNAME:[[시즈카]]%
IF (TFLAG:불리전투회화스위치 & 1) != 0 || (TFLAG:불리전투회화스위치 & 2) != 0
    ;체력이 상당히 떨어짐
    LTEXT = %BUDDY([[시즈카]])%!
    FACEV = 4
    TFLAG:전투회화키워드1 = 1
ELSEIF TFLAG:전투경과시간회화 > 1
    ;전투에서 시간을 상당히 오래 끔
    LTEXT = 하아...하아...
    FACEV = 3
    TFLAG:전투회화키워드1 = 5
ELSEIF TFLAG:경과시간 < 60000
    ;1분 컷
    LTEXT = 어, 벌써 끝났나?
    FACEV = 1
    TFLAG:전투회화키워드1 = 3
ELSE
    ;통상
    LTEXT = 이겼더니 배가 고파졌다...
    FACEV = 3
    TFLAG:전투회화키워드1 = 6
ENDIF
CALL TALK_WITH_IMG(FACE([[시즈카]],FACEV),LNAME,LTEXT)
WAITANYKEY
RETURN 1
@KOJO_BATTLE_WIN_RES_K15
#DIMS DYNAMIC LNAME
#DIMS DYNAMIC LTEXT
#DIMS DYNAMIC LTEXT2
#DIM DYNAMIC FACEV = 1
LNAME = %CALLNAME:[[시즈카]]%
IF (TFLAG:불리전투회화스위치 & 1) != 0 || (TFLAG:불리전투회화스위치 & 2) != 0
    SELECTCASE TFLAG:전투회화키워드1
    ;체력이 상당히 떨어짐
        CASE 5
            ;피로
            FACEV = 4
            IF TFLAG:전투회화키워드2 == 2
                ;허세
                LTEXT = %BUDDY([[시즈카]])%...
            ELSE
                IF JONDAE([[시즈카]],PARTNER([[시즈카]]))
                    LTEXT = 무리하면 안 돼요, %BUDDY([[시즈카]])%
                ELSE
                    LTEXT = %BUDDY([[시즈카]])%, 너무 무리하면 안 돼.
                ENDIF
            ENDIF
        CASEELSE
            ;기타
            LTEXT = 하아... 하아...
            FACEV = 5
    ENDSELECT
ELSEIF TFLAG:전투경과시간회화 > 1
    ;전투에서 시간을 상당히 오래 끔
    SELECTCASE TFLAG:전투회화키워드1
        CASE 4
            ;거만
            IF JONDAE([[시즈카]],PARTNER([[시즈카]]))
                FACEV = 5
                LTEXT = 주변 경계를 소홀히 하면 안 돼요.
            ELSE
                FACEV = 1
                LTEXT = 너무 까불다간 혼난다.
            ENDIF
        CASEELSE
            ;기타
            FACEV = 4
            LTEXT = 후... 이걸로 전부?
    ENDSELECT
ELSEIF TFLAG:경과시간 < 60000
    ;1분 컷
    SELECTCASE TFLAG:전투회화키워드1
        CASE 3
            ;장난
            IF JONDAE([[시즈카]],PARTNER([[시즈카]]))
                FACEV = 4
                LTEXT = 무슨 소리에요 그게...
            ELSE
                FACEV = 3
                LTEXT = 장난치지 말고, %BUDDY([[시즈카]])%
            ENDIF
        CASEELSE
            ;기타
            FACEV = 1
            IF JONDAE([[시즈카]],PARTNER([[시즈카]]))
                LTEXT = 하던 일로 돌아가죠.
            ELSE
                LTEXT = 하던 일로 돌아가자.
            ENDIF
    ENDSELECT
ELSE
    ;통상
    SELECTCASE TFLAG:전투회화키워드1
        CASE 1
            ;걱정
            IF JONDAE([[시즈카]],PARTNER([[시즈카]]))
                LTEXT = 저는 괜찮아요.
                FACEV = 2
            ELSE
                LTEXT = 난 괜찮아.
            ENDIF
        CASE 2
            ;웃음
            FACEV = 1
            LTEXT = 후후, %BUDDY([[시즈카]])%...
        CASEELSE
            ;기타
            FACEV = 1
            IF JONDAE([[시즈카]],PARTNER([[시즈카]]))
                LTEXT = 다음에 대비하죠.
            ELSE
                LTEXT = 다음에 대비하자.
            ENDIF
    ENDSELECT
ENDIF
CALL TALK_WITH_IMG(FACE([[시즈카]],FACEV),LNAME,LTEXT)
WAITANYKEY
RETURN 1
;패배
@KOJO_BATTLE_LOSE_K15
#DIMS DYNAMIC LNAME
#DIMS DYNAMIC LTEXT
#DIM DYNAMIC FACEV = 3
LNAME = %CALLNAME:[[시즈카]]%
SELECTCASE FACEV
    CASE 3
        LTEXT = 여기까지인가봐...
        FACEV = 3
        SIF PARTNER([[시즈카]]) != [[미라이]]
            LTEXT += BYNAMES([[시즈카]],[[미라이]]) + "... "
        SIF PARTNER([[시즈카]]) != [[츠바사]]
            LTEXT += MIXELLIPSIS(BYNAMES([[시즈카]],[[츠바사]]))
        CALL TALK_WITH_IMG(FACE([[시즈카]],FACEV),LNAME,LTEXT)
        WAITANYKEY
        LTEXT = %MIXELLIPSIS(BUDDY([[시즈카]]))%...
        TFLAG:전투회화키워드1 = 8
    CASE 1
        LTEXT = 안돼, 이젠...
        FACEV = 3
        TFLAG:전투회화키워드1 = 8
    CASE 2
        LTEXT = %MIXELLIPSIS(BUDDY([[시즈카]]))%... 도...망쳐...
        FACEV = 3
        TFLAG:전투회화키워드1 = 9
ENDSELECT
CALL TALK_WITH_IMG(FACE([[시즈카]],FACEV),LNAME,LTEXT)
WAITANYKEY
RETURN 1
