                LTEXT = 적들이에요!
            ELSE
                LTEXT = 적들이야!
            ENDIF
    ENDSELECT
    CALL TALK_WITH_IMG(FACE([[시즈카]],5),LNAME,LTEXT)
    WAITANYKEY
    RETURN 1
ENDIF
