;캐릭 CSV번호를 인수로 받아 드롭아이템 결정
@DROP_ITEM(NOV)
#DIM DYNAMIC NOV
#DIM DYNAMIC ITEMNUM

ITEMNUM = -1
SELECTCASE NOV
    CASE 100
        ;츄파카브라
        SELECTCASE RAND:100
            CASE 0 TO 16
                ITEMNUM = [[츄파에센스]]
            CASE 17 TO 40
                ITEMNUM = [[스파크드링크10]]
        ENDSELECT
ENDSELECT
RETURN ITEMNUM