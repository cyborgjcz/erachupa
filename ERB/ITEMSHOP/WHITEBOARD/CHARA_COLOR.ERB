@MIX_COLOR(COLOR0,COLOR1)
#FUNCTION
#DIM DYNAMIC COLOR0
#DIM DYNAMIC COLOR1
#DIM DYNAMIC CODEV
#DIM DYNAMIC REDV,2
#DIM DYNAMIC GREENV,2
#DIM DYNAMIC BLUEV,2

REPEAT 2
	SELECTCASE COUNT
		CASE 0
			CODEV = COLOR0
		CASE 1
			CODEV = COLOR1
	ENDSELECT
	REDV:COUNT = CODEV / 0x010000
	GREENV:COUNT = (CODEV % 0x010000) / 0x000100
	BLUEV:COUNT = (CODEV % 0x010000) % 0x000100 
REND 

RETURNF (SUMARRAY(REDV)/2)*65536 + (SUMARRAY(GREENV)/2)*256 + (SUMARRAY(BLUEV)/2)

